{
  "name": "gcp-iam-completions",
  "publisher": "patrickdoc",
  "displayName": "GCP IAM Completions",
  "description": "Completions for all predefined GCP IAM roles",
  "author": {
    "name": "Patrick Dougherty",
    "email": "patrick.dougherty.0208@gmail.com"
  },
  "license": "MIT",
  "version": "1.1.13",
  "bugs": "https://github.com/patrickdoc/vscode-gcp-iam-completions/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickdoc/vscode-gcp-iam-completions"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "gcp-iam-completions.diffRoles",
        "title": "GCP IAM: Diff roles"
      }
    ]
  },
  "categories": [
    "Snippets"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "keywords": [
    "gcp",
    "autocompletion"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "deploy": "vsce publish",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "save-roles": "ts-node -e \"import { saveRoles } from './src/google/iam'; saveRoles()\""
  },
  "devDependencies": {
    "@googleapis/iam": "^11.0.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.8",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.20.1",
    "eslint": "^8.46.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "googleapis": "^123.0.0"
  }
}
